{
  "title": "OCG System Health",
  "schemaVersion": 39,
  "panels": [
    {
      "type": "timeseries",
      "title": "HTTP Request Rate",
      "targets": [
        {
          "expr": "sum(rate(http_requests_total[5m])) by (route)"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Latency p95",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, route))"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Unknown Permission Records",
      "targets": [
        {
          "expr": "increase(permission_unknown_records_total[5m])"
        }
      ]
    }
  ]
}

